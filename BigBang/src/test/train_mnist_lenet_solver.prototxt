mode: GPU
#93720
train_iterations: 93720
validate_iterations: 10
test_validatedata_accuracy_per_train_iterations: 500
train_batch_size: 32
validate_batch_size: 1000
lr: 0.003
weight_decay_param: {
	method: L2REGULARIZATION,
	alpha: 0.0001
}
net_param {
	name: "train_minst"
  state: TRAIN
  layer_param {
    name: "data"
    type: "Data"
    data_layer_param {
      batch_size: 32
      cache_batch_count: 10
      source: "D:/deeplearning/mnist_lmdb/mnist_train.mdb"
			preprocess_params: {
				scale: 0.00392156
			}
    }
  }
  layer_param {
    name: "Conv1"
    type: "Conv"
    conv_layer_param {
      kernel_groups: 20
			kernel_channels: 1
			kernel_h: 5
			kernel_w: 5
			stride_h: 1
			stride_w: 1
      use_bias: true
      kernel_filler: {
        type: XAVIER
				std: 0.01
      }
      bias_filler: {
        type: GAUSSIAN_DISTRIBUTION
      }
    }
  }
  layer_param {
    name: "Pool1"
    type: "Pooling"
		pooling_layer_param {
			kernel_h: 2
			kernel_w: 2
			stride_h: 2
			stride_w: 2
		}
  }
	layer_param {
    name: "Conv2"
    type: "Conv"
    conv_layer_param {
      kernel_groups: 50
			kernel_channels: 20
			kernel_h: 5
			kernel_w: 5
			stride_h: 1
			stride_w: 1
      use_bias: true
      kernel_filler: {
        type: XAVIER
				std: 0.01
      }
      bias_filler: {
        type: GAUSSIAN_DISTRIBUTION
      }
    }
  }
  layer_param {
    name: "Pool1"
    type: "Pooling"
		pooling_layer_param {
			kernel_h: 2
			kernel_w: 2
			stride_h: 2
			stride_w: 2
		}
  }
	layer_param {
		name: "dropout1"
		type: "Dropout"
		dropout_layer_param {
			dropout_ratio: 0.5
		}
	}
  layer_param {
    name: "inner1"
    type: "InnerProduct"
    inner_product_layer_param {
      output_nums: 500
      use_bias: true
      weight_filler: {
        type: XAVIER
				std: 0.01
      }
      bias_filler: {
        type: GAUSSIAN_DISTRIBUTION
      }
    }
  }
	layer_param {
		name: "dropout1"
		type: "Dropout"
		dropout_layer_param {
			dropout_ratio: 0.5
		}
	}
  layer_param {
    name: "neuron1"
    type: "Relu"
  }
	layer_param {
    name: "inner2"
    type: "InnerProduct"
    inner_product_layer_param {
      output_nums: 10
      use_bias: true
      weight_filler: {
        type: XAVIER
				std: 0.01
      }
      bias_filler: {
        type: GAUSSIAN_DISTRIBUTION
      }
    }
  }
  layer_param {
    name: "end"
    type: "SoftmaxCost"
  }
}

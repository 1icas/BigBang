	name: "test_minst"
  state: VALIDATE
  layer_param {
    name: "data"
    type: "Data"
    data_layer_param {
      batch_size: 1000
      cache_batch_count: 1
      source: "D:/deeplearning/mnist_lmdb/mnist_test.mdb"
			preprocess_params: {
				scale: 0.00392156
			}
    }
  }
  layer_param {
    name: "Conv1"
    type: "Conv"
    conv_layer_param {
      kernel_groups: 20
